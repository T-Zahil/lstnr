{
  "connection": "default",
  "info": {
    "name": "user",
    "description": ""
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "points": {
      "max": 9999,
      "type": "integer"
    },
    "profile-picture": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "company": {
      "maxLength": 255,
      "minLength": 2,
      "type": "string",
      "required": true
    },
    "company-position": {
      "maxLength": 255,
      "minLength": 2,
      "type": "string",
      "required": true
    },
    "bio": {
      "maxLength": 500,
      "type": "text"
    },
    "comments": {
      "collection": "comment",
      "via": "author"
    },
    "ideas": {
      "collection": "idea",
      "via": "author"
    },
    "products": {
      "collection": "product",
      "via": "owner"
    }
  },
  "collectionName": "users-permissions_user"
}